    <script type="dcc/template" id="dcc-dropdown">
        <div class="dropdown--wrapper {{ class }}">
            {{#if label }}
            <div class="label-wrapper-refine">
                <span class="icon-list_expand" style="display: none;">
                    <svg class="svg-pdp-list_expand">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#pdp-list_expand"></use>
                    </svg>
                </span>
                <span class="icon-list_collapse">
                    <svg class="svg-pdp-list_collapse">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#pdp-list_collapse"></use>
                    </svg>
                </span>
                <label for="{{ id }}">{{ label }}</label>
            </div>
            {{/if}}
            <div class="dropdown {{#if forSelect }}with_select{{/if}} {{#if scrollable }}dropdown-menu--scrollable{{/if}}">

                {{#if text }}
                <button id="product-search-page-compound-results" class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span class="dropdown__text">{{ text }}</span>
                    <span class="dropdown__arrow--down">
                    <svg class="svg-dropdown_arrow--down">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#dropdown_arrow--down"></use>
                    </svg>
                    </span>
                </button>
                {{#if deleteBtn }}
                <span class="dropdown-delete hidden">
                    <svg class="svg-x_outline">
                      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#x_outline"></use>
                    </svg>
                </span> {{/if}}
                {{/if}} {{#if icon }}
                <a href="#" class="dropdown-toggle primary deepLink" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                    <span class="button--label hidden-sm hidden-md hidden-lg">{{ mobileText }}</span>
                    <span class="icon hidden-sm hidden-xs hidden-md hidden-lg">
                    <svg class="svg-nav-right-arrow--mobile">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav-right-arrow--mobile"></use>
                    </svg>
                    </span>
					<span class="nav-icon-circle hidden-sm hidden-md hidden-lg">
							<svg class="svg-header_plus">
									<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#header_plus"></use>
								</svg>
								<svg class="svg-header_minus">
									<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#header_minus"></use>
								</svg>
							</span>
                    <svg class="svg-{{ icon }} hidden-xs">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#{{ icon }}"></use>
                    </svg>
                </a>
                <a href="#" role="button" class="hidden secondary deepLink">
                    <span class="icon hidden-sm hidden-md hidden-lg">
                        <svg class="svg-nav-right-arrow--mobile">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav-right-arrow--mobile"></use>
                        </svg>
                    </span>
                    <span class="button--label"></span>
                </a>
                <span class="underline"></span> {{/if}}
                <ul class="dropdown-menu locations-dropdown language-new-dropdown {{#if scrollable }}dropdown-menu--scrollable{{/if}}" aria-labelledby="{{ id }}" role="menu">
                    {{#each languages as |item| }}
                    <li>
                        <p><a href="javascript:void(0);" data-languagecode="{{#if item.languageCode }}{{ item.languageCode }}{{else}}#{{/if}}">{{ item.value }}</a></p>
                    </li>
                    {{/each}} {{#each values as |item| }}
                    <li>
                        <p><a href="{{#if item.link }}{{ item.link }}{{else}}#{{/if}}" {{#if @root.forSelect }}data-trigger-select="{{#if item.isocode }}{{ item.isocode }}{{else}}{{ item.value }}{{/if}}" {{/if}} {{#if item.viewall }}data-viewall="true"
                            {{/if}} {{#if item.selected }}class="selected" {{/if}} data-callback="{{#if item.postCodeAvailable }}regionSelection{{/if}}" data-languagecode="{{#if item.languageCode }}{{ item.languageCode }}{{else}}#{{/if}}">{{ item.value }}{{#ifCond item.count '&&' @root.showCount }} ({{ item.count }}){{/ifCond}}</a></p>
                    </li>
                    {{/each}} {{#if separated_items }}
                    <li role="separator" class="divider"></li>
                    {{/if}} {{#each separated_items as |item| }}
                    <li>
                        <p><a href="{{#if item.link }}{{ item.link }}{{else}}#{{/if}}" {{#if @root.forSelect }}data-trigger-select="{{#if item.isocode }}{{ item.isocode }}{{else}}{{ item.value }}{{/if}}" {{/if}} {{#if item.viewall }}data-viewall="true"
                            {{/if}} {{#if item.selected }}class="selected" {{/if}} data-callback="{{#if item.postCodeAvailable }}regionSelection{{/if}}" data-languagecode="{{#if item.languageCode }}{{ item.languageCode }}{{else}}#{{/if}}">{{ item.value }}{{#ifCond item.count '&&' @root.showCount }} ({{ item.count }}){{/ifCond}}</a></p>
                    </li>
                    {{/each}}
                </ul>
            </div>
            <!--end dropdown -->
            {{#if forSelect}}
            <select {{#if id}}name="{{ id }}" id="{{ id }}" {{/if}} class="dropdown-select" {{#if required }}required{{/if}}>
            <option value=" ">{{text}}</option>
            {{#each values as |item| }}
            <option value="{{#if item.isocode }}{{ item.isocode }}{{else}}{{ item.value }}{{/if}}">{{ item.value }}</option>
            {{/each}}
            </select> {{/if}}
        </div>
        <!--end dropdown wrapper -->
        {{#if infoText }}
        <p class="dropdown--info-text">{{ infoText }}</p>
        {{/if}}
    </script>
