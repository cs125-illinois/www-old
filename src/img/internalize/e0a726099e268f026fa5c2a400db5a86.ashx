        <script type="text/template" id="notification_modal">
            <div class="modal fade {{unique_id}}" id="{{unique_id}}" role="dialog">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header {{type}}">
                            <h3 class="modal-title">{{title}}</h3>
                            <button class="close" data-dismiss="modal">

                                <svg class="svg-nav_close_x">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav_close_x"></use>
                                </svg>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-xs-12">
                                    {{message}}
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer {{type}}">
                            <div class="row">
                                <div class="col-xs-8"></div>
                                <div class="col-xs-4">
                                    <div class="component-pdpAddPartNumbers-footer-button-holder cancel-button">
                                        <button type="button" class="btn btn-block  btn-info">{{button_text}}</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </script>

        <script type="text/template" id="notification_inpage">
            <div id="{{ id }}" class="component-notification {{ type }}">
                <div class="notification_icon">
                    <div class="notification_icon_wrapper">
                        {{#if_eq type 'danger'}}


                        <svg class="svg-nav_exclamation_point">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav_exclamation_point"></use>
                        </svg> {{/if_eq}} {{#if_eq type 'success'}}


                        <svg class="svg-nav_check_mark">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav_check_mark"></use>
                        </svg> {{/if_eq}} {{#if_eq type 'info'}}


                        <svg class="svg-nav_info">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#nav_info"></use>
                        </svg> {{/if_eq}}
                    </div>
                </div>
                <div class="notification_content">
                    <div class="notification_content_inner">
                        <span class="notification_highlighted">{{highlighted_text}}</span> &nbsp; {{text}} {{#each buttons}}
                        <button type="button" class="btn btn-block {{button.type}}">
                            {{button.text}}
                        </button> {{/each}}
                    </div>
                </div>
            </div>
        </script>
        <!-- NOTIFICATIONS SCRIPT -->
    
    
