{{#each redirects}}
rewrite ^{{{ from }}}$ {{{ to }}} redirect;
rewrite ^{{{ from }}}/$ {{{ to }}} redirect;
{{/each}}
{{#each slideRedirects}}
rewrite ^{{{ from }}}$ {{{ to }}} redirect;
rewrite ^{{{ from }}}/$ {{{ to }}} redirect;
{{/each}}
rewrite ^/static-assets/main/site.css$ {{ lookup webpack.assets 'site.css' }} redirect;
rewrite ^/static-assets/main/site.js$ {{ lookup webpack.assets 'site.js' }} redirect;
rewrite ^/static-assets/css/site.css$ {{ lookup webpack.assets 'site.css' }} redirect;
rewrite ^/static-assets/js/site.js$ {{ lookup webpack.assets 'site.js' }} redirect;
rewrite ^/info/2018/fall/fair/ /info/fair/ redirect;
rewrite ^/info/2018/fall/fair /info/fair/ redirect;
